{
    "name": "apisnetworks/cp-collect",
    "description": "ApisCP site collector.",
    "keywords": ["apiscp", "proxy", "console", "cli"],
    "homepage": "https://apiscp.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/apisnetworks/cp-collect/issues",
        "source": "https://github.com/apisnetworks/cp-collect"
    },
    "authors": [
        {
            "name": "Matt Saladna",
            "email": "matt@apisnetworks.com"
        }
    ],
    "require": {
        "php": "^7.3|^8",
        "clue/soap-react": "^2.0",
        "illuminate/database": "^8",
        "illuminate/encryption": "^8.13",
        "illuminate/log": "^8",
        "laravel-zero/framework": "^8",
        "nunomaduro/laravel-console-menu": "^3.1",
        "react/child-process": "^0.6.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "doctrine/dbal": "^2.12",
        "fakerphp/faker": "^1.20",
        "illuminate/view": "^8.11",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["proxy"]
}
